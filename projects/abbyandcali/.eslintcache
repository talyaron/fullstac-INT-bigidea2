div>\n            <h4 id=\"sentenceHeader\">Sentences</h4>\n            <div id=\"sentencesDisplay\"></div>\n            {\n                sentences.map((sentence, index) => {\n                    return (<p key={index} className=\"sentence\" onClick={() => handleSentenceClick(`${sentence.text}`)}>{sentence.text}</p>)\n                })\n            }\n            <div><Link to=\"/ListDoc\" onClick={handleSelectionSubmit} id=\"linkClick\">click here to finalize your selection</Link></div>\n            <div id=\"listImport\"></div>\n\n        </div>\n\n\n\n    );\n\n}\n\n\nexport default SelectDoc;\n","/Users/abbymiller/Documents/github/fullstac-INT-bigidea2/projects/abbyandcali/src/Components/Send/SendDoc.js",[],["45","46"],"/Users/abbymiller/Documents/github/fullstac-INT-bigidea2/projects/abbyandcali/src/Components/FirebaseConfig.js",[],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":24,"column":8,"nodeType":"53","endLine":24,"endColumn":10,"suggestions":"54"},{"ruleId":"55","severity":1,"message":"56","line":29,"column":13,"nodeType":"57","endLine":29,"endColumn":57},{"ruleId":"58","severity":1,"message":"59","line":4,"column":8,"nodeType":"60","messageId":"61","endLine":4,"endColumn":12},{"ruleId":"58","severity":1,"message":"62","line":6,"column":22,"nodeType":"60","messageId":"61","endLine":6,"endColumn":28},{"ruleId":"58","severity":1,"message":"63","line":11,"column":5,"nodeType":"60","messageId":"61","endLine":11,"endColumn":16},{"ruleId":"58","severity":1,"message":"64","line":12,"column":5,"nodeType":"60","messageId":"61","endLine":12,"endColumn":18},{"ruleId":"47","replacedBy":"65"},{"ruleId":"49","replacedBy":"66"},"no-native-reassign",["67"],"no-negated-in-lhs",["68"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'emojiURL', 'feeling', and 'message'. Either include them or remove the dependency array.","ArrayExpression",["69"],"jsx-a11y/alt-text","img